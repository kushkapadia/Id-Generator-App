  </div class="IDFormdiv"> 
    <form action="/generate" method="POST" class="IDForm">

        <div>

         <!-- <img src="/images/test.png" alt="">  -->

          <div>
            <label for="Name">Name:</label>
            <input name="Name" class="Name" type="text" autocomplete="off" required>
          </div>
          
          <div>
            <label for="EName1">Emergency Contact Name 1:</label>
            <input name="EName1" class="EName1" type="text" required>
          </div>

          <div>
           <label for="phone1">Emergency Number 1:</label>
        <input type="tel" class="phone1" name="phone1" required>
          </div>

          <div>
            <label for="EName2">Emergency Contact Name 2:</label>
            <input name="EName2" class="EName2" type="text">
          </div>

          <div>
            <label for="phone2">Emergency Number 2:</label>
         <input type="tel" class="phone2" name="phone2">
           </div>

          <div>
            <label for="bg">Blood Group:</label>
            <input name="bg" class="bg"  type="text" required>
          </div>
          
          <div>
            <label for="birthYear">Birth Date: </label>
            <input name="birthYear"  class="birthYear"  type="date" required>
          </div>
          
          <div>
            <label for="pinCode">Pin Code </label>
            <input name="pinCode"  class="pinCode"  type="text"
            minlength="4" required>
          </div>
          
          <input type="submit" value="Generate Id"> 
        </form>
     <div>
            <div class="parent" id="parent"> </div>
         
        </body>
        
        


       

